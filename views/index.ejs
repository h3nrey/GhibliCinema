<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ghibli Cinema</title>

    <style></style>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600;700&family=Poppins:wght@500;600;700;800&display=swap" rel="stylesheet"> 
    <link rel="stylesheet" href="css/main.css">

    <link rel="shortcut icon" href="https://64.media.tumblr.com/f2a13ff3959cf8280f1a81765fc3f29d/4cd086da613a1d0d-75/s1280x1920/8fbc83cae3df21e4a84fd0d915b94afef730f135.png" type="image/x-icon">

    
<body>
    <div id="container">
        <div class="content">
            <header>
                <div id="logo-container">
                    <a href="#" id="Logo">Ghibli Cinema</a>
                </div>
                <div id="menu">
                    <a href="">Filmes</a>
                    <a href="">Curtas</a>
                    <a href="">Hist√≥ria</a>
                </div>
            </header>
            <main>
                <div class="cards">
                    <h2>Melhores Filmes</h2>

                    <div class="arrow-left">
                        <img src="assets/l-arrow.png">
                        </div>
    
                        <div class="arrow-right">
                            <img src="assets/r-arrow.png" alt="seta">
                        </div>

                    <div class="cardsArea">
                        <div class="VerticallList">
                            <% movies.forEach(movie => { %>
                                    <a href=<%= `/movie/${movie.id}`%> class="VerticalCard">
                                        <img src= <%= movie.image%>  alt= <%= movie.tittle%> >
                                    </a>
                            <% }) %>
                        </div>
                    </div>
                </div>
                  
                <div class="cards">
                    <h2>Hayao Myiazaki</h2>

                    <div class="arrow-left-horizontal" id="h-l-arrow" readonly>
                    <img src="assets/l-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="arrow-right-horizontal" id="h-r-arrow" readonly>
                        <img src="assets/r-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="cardsArea">
                        <div class="cardsList">
                            <% movies.forEach(movie => { %>
                                <% if (movie.director == "Hayao Miyazaki") { %>
                                    <a href=<%= `movie/${movie.id}`%> class="HorizontalCard">
                                        <img src= <%= movie.movie_banner%>  alt= <%= movie.tittle%> id="photo">
                                    </a>
                                <% } %>
                            <% }) %>                            
                        </div>
                        
                    </div>                 
                </div>

                <div class="cards">
                    <h2>Isao Takahata</h2>

                    <div class="arrow-left-horizontal" id="i-l-arrow" readonly>
                    <img src="assets/l-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="arrow-right-horizontal" id="i-r-arrow" readonly>
                        <img src="assets/r-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="cardsArea">
                        <div class="cardsList">
                            <div class="cardsList">
                                <% movies.forEach(movie => { %>
                                    <% if (movie.director == "Isao Takahata") { %>
                                        <a href=<%= `movie/${movie.id}`%> class="HorizontalCard">
                                            <img src= <%= movie.movie_banner%>  alt= <%= movie.tittle%> id="photo">
                                        </a>
                                    <% } %>
                                <% }) %>   
                        </div>
                    </div>                 
                </div>
                
                <div class="cards">
                    <h2>Outros</h2>

                    <div class="arrow-left-horizontal" id="o-l-arrow" readonly>
                    <img src="assets/l-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="arrow-right-horizontal" id="o-r-arrow" readonly>
                        <img src="assets/r-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="cardsArea">
                        <div class="cardsList">
                            <% movies.forEach(movie => { %>
                                <% if (movie.director != "Isao Takahata" && movie.director != "Hayao Miyazaki") { %>
                                    <a href=<%= `movie/${movie.id}`%> class="HorizontalCard">
                                        <img src= <%= movie.movie_banner%>  alt= <%= movie.tittle%> id="photo">
                                    </a>
                                <% } %>
                            <% }) %> 
                        </div>
                    </div>                 
                </div>

                <div class="cards">
                    <h2>Curtas</h2>

                    <div class="arrow-left-horizontal" id="s-l-arrow" readonly>
                    <img src="assets/l-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="arrow-right-horizontal" id="s-r-arrow" readonly>
                        <img src="assets/r-arrow.png" alt="seta" readonly>
                    </div>

                    <div class="cardsArea">
                        <div class="cardsList">
                            <a href="" class="HorizontalCard">
                                <img  src="https://studioghibli.com.br/wp-content/uploads/2020/07/On-Your-Mark.jpg" alt="On your mark">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQvTbmOZvzyX0nZzfBF6gcRKnS7t2Q67EkZlw&usqp=CAU" alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="https://lh3.googleusercontent.com/proxy/Y-5PHSiZLn4ox0utVFO_L_FAV7a2F8SBsjdH7lnsTqUTCa4YS_1npwaqorm8j3zJZTCSZAkn6SBvXirCZFr5n2eqlIMfJMYU5hNMTwgwH0cuzoPlpcj_VJ7BUz8" alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img src="data:image/jpeg;base64," alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="data:image/jpeg;base64," alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="data:image/jpeg;base64," alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="https://media.comicbook.com/2021/04/spirited-away-1266420-1280x0.jpeg" alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="https://www.thegoldentake.com/wp-content/uploads/%C2%A9-2004-Studio-Ghibli-howls-moving-castle.jpg" alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="https://cdn.abcdoabc.com.br/Educacao-para-todos-1_3a86d585.jpg" alt="">
                            </a>
                            <a href="" class="HorizontalCard">
                                <img  src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTdinWdLA5xPnw2gY5ap9JYFK-xBXRfTxfuuQ&usqp=CAU" alt="">
                            </a>

                        </div>
                    </div>                 
                </div>

            </main>
        </div>        
    </div>

    <script src="scripts/scrooll.js"></script>
</body>
</html>